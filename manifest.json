{
    "manifest_version": 2,
    "name": "Motivation",
    "version": "1.0",
    "description": "A chrome extension that motivates you to get back to work when you load a distracting website.",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "https://www.reddit.com/*",
                "https://www.tumblr.com/*"],
            "js": ["./main.js"]
        }
    ]
}